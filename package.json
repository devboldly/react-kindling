{
  "name": "@devboldly/react-library-starter",
  "version": "0.0.0",
  "author": "Dev Boldly <devboldly@gmail.com>",
  "description": "A template to build, test, and share React components and hooks easily. Uses Storybook, Jest, and more.",
  "homepage": "https://github.com/devboldly/react-library-starter#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "npm run build && git add dist && git diff-index --quiet HEAD || git commit -m \"Build and deploy.\" && git push && npm version patch -m \"Build and deploy.\"",
    "deploy": "npm publish --access public",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "docz:clean": "rm -rf .docz/.cache"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devboldly/react-library-starter.git"
  },
  "bugs": {
    "url": "https://github.com/devboldly/react-library-starter/issues"
  },
  "keywords": [],
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-notes": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "babel-loader": "^8.0.6",
    "docz": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-json": "^2.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-app": "^6.2.1",
    "eslint-plugin-react-hooks": "^2.5.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.0.0-beta.2",
    "react-test-renderer": "^16.13.0",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  }
}
